  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-angular": "16.2.1",
    "eslint": "^9.25.1",
    "eslint-config-nodebb": "^1.1.4",
    "eslint-plugin-import": "^2.31.0",
    "husky": "7.0.4",
    "lint-staged": "12.3.5"
