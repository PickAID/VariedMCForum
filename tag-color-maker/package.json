{
  "name": "nodebb-plugin-tag-color-maker",
  "version": "1.0.0",
  "description": "A NodeBB plugin to generate tag colors for posts",
  "main": "library.js",
  "author": "NodeBB",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/NodeBB/nodebb-plugin-markdown-toc"
  },
  "bugs": {
    "url": "https://github.com/NodeBB/nodebb-plugin-markdown-toc/issues"
  },
  "homepage": "https://github.com/NodeBB/nodebb-plugin-markdown-toc",
  "dependencies": {
    "cheerio": "^1.0.0-rc.12"
  },
  "peerDependencies": {
    "nodebb": ">=1.0.0"
  },
  "keywords": [
    "nodebb",
    "plugin",
    "markdown",
    "toc",
    "table-of-contents"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "nbbpm": {
    "compatibility": "^3.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "19.8.1",
    "@commitlint/config-angular": "19.8.1",
    "eslint": "9.x",
    "eslint-config-nodebb": "1.1.4",
    "eslint-plugin-import": "2.x",
    "husky": "9.1.7",
    "lint-staged": "16.0.0"
  }
}
